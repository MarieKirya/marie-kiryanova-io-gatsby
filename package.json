{
  "name": "marie-kiryanova-io-gatsby",
  "private": true,
  "description": "Front end to the D8 CMS for my portfolio",
  "version": "0.1.0",
  "author": "Marie Kiryanova <marie@kiryanova.io>",
  "dependencies": {
    "@mdi/font": "^6.9.96",
    "@mdi/js": "^6.9.96",
    "@mdi/react": "^1.6.0",
    "bulma": "^0.9.4",
    "gatsby": "^4.17.2",
    "gatsby-plugin-image": "^2.17.0",
    "gatsby-plugin-advanced-sitemap": "^2.1.0",
    "gatsby-plugin-google-analytics": "^4.17.0",
    "gatsby-plugin-manifest": "^4.17.0",
    "gatsby-plugin-netlify": "^5.0.0",
    "gatsby-plugin-offline": "^5.17.0",
    "gatsby-plugin-react-helmet": "^5.17.0",
    "gatsby-plugin-sharp": "^4.17.0",
    "gatsby-source-drupal": "^5.18.0",
    "gatsby-source-filesystem": "^4.17.0",
    "gatsby-transformer-sharp": "^4.17.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^8.0.3"
  },
  "devDependencies": {
    "sass": "^1.53.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0",
    "prettier": "2.7.1"
  },
  "keywords": [
    "portfolio",
    "decoupled-drupal",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run css-build && npm run css-postcss && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass src/scss/main.scss src/css/main.css",
    "css-postcss": "postcss --use autoprefixer --output src/css/main.css src/css/main.css",
    "css-watch": "npm run css-build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
