{
  "name": "marie-kiryanova-io-gatsby",
  "private": true,
  "description": "Front end to the D8 CMS for my portfolio",
  "version": "0.1.0",
  "author": "Marie Kiryanova <marie@kiryanova.io>",
  "dependencies": {
    "@mdi/font": "^3.6.95",
    "@mdi/js": "^3.6.95",
    "@mdi/react": "^1.1.0",
    "bulma": "^0.7.4",
    "gatsby": "^2.3.33",
    "gatsby-image": "^2.0.40",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-source-drupal": "^3.1.4",
    "gatsby-source-filesystem": "^2.0.32",
    "gatsby-transformer-sharp": "^2.1.18",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.0.8"
  },
  "devDependencies": {
    "prettier": "^1.17.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.1"
  },
  "keywords": [
    "portfolio",
    "decoupled-drupal",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run css-build && npm run css-postcss && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass src/scss/main.scss src/css/main.css",
    "css-postcss": "postcss --use autoprefixer --output src/css/main.css src/css/main.css",
    "css-watch": "npm run css-build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
